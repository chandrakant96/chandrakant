<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dialogCoordinatorLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:fitsSystemWindows="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imgWhrLogo"
                android:layout_width="@dimen/_80sdp"
                android:layout_height="@dimen/_80sdp"
                android:layout_centerHorizontal="true"
                android:background="@color/white"
                android:src="@drawable/ic_action_whr_log" />

            <com.balysv.materialripple.MaterialRippleLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                app:mrl_rippleAlpha="0.2"
                app:mrl_rippleColor="@color/primary"
                app:mrl_rippleDelayClick="false"
                app:mrl_rippleHover="true"
                app:mrl_rippleOverlay="true">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/imageClose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/_4sdp"
                    android:src="@drawable/ic_close_white"
                    app:civ_border_color="@color/primary"
                    app:civ_border_width="@dimen/_2sdp" />

            </com.balysv.materialripple.MaterialRippleLayout>

        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/_1sdp"
            android:background="@color/divider" />
        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="मुख्यमंत्री सहाय्यता निधी : ०२२-२२०२६९४८ "
            android:textSize="@dimen/_14sdp" />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginTop="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="गर्भधारणापूर्व व प्रसवपूर्व निदान तंत्र टोल फ्री दूरध्वनी क्र : 18002334475 "
            android:textSize="@dimen/_14sdp" />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="राजीव गांधी जीवनदाई आरोग्य योजना टोल फ्री दूरध्वनी क्र : 18002332200 "
            android:textSize="@dimen/_14sdp" />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:textSize="@dimen/_14sdp"
            android:text="जननी शिशु सुरक्षा कार्यक्रम - मोफत संदर्भ सेवा वाहतूक, टोल फ्री दूरध्वनी क्र : १०२ " />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="आरोग्य सल्ला संपर्क केंद्र दूरध्वनी क्र : १०४ "
