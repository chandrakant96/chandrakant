            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="एकात्मिक रोग सर्वेक्षण प्रकल्प टोल फ्री दूरध्वनी क्र : १०७५"
            android:textSize="@dimen/_14sdp" />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="ब्लड ऑन कॉल टोल फ्री दूरध्वनी क्र : १०४"
            android:textSize="@dimen/_14sdp" />
        <View
            android:layout_width="match_parent"
            android:layout_margin="@dimen/_5sdp"
            android:layout_height="1dp"
            android:background="@color/divider" />

        <com.whr.user.com.WHR.ViewPackage.OpenSansRegular
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/_2sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_10sdp"
            android:textColor="@color/black"
            android:text="अकस्मात वैद्यकीय सेवा टोल फ्री दूरध्वनी क्र : १०८."
